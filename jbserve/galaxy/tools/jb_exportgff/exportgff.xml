<tool id="jb_exportgff" name="Export to JBlast" version="1.0.0">
  <description>Export results to JBlast Module</description>
  <command>jblast-export -t ./inMemTemplate.json -g $gffin -b $jsonin -o $output</command>
  <inputs>
    <param format="gff" name="gffin" type="data" label="GFF in"/>
    <param format="json" name="jsonin" type="data" label="blastJSON in"/>
  </inputs>
  <outputs>
      <data format="json" name="output" label="jb export results (JSON)"/>
</outputs>

  <help>
Galaxy Script by: ey 5/11/2016 
  </help>

</tool>